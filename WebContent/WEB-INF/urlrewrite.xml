<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"  
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>

	<!-- 用于重写url -->
	<rule>
		<from>/default.url</from>
		<to>/frontIndex!toDefault.action</to>
	</rule>

	<rule>
		<from>/an.url</from>
		<to>/frontAnnouncement!announcementList.action</to>
	</rule>
	<rule>
		<from>/san.url</from>
		<to>/frontAnnouncement!getSingleAnnouncement.action</to>
	</rule>

	<rule>
		<from>/fa.url</from>
		<to>/frontAnnouncement!favorList.action</to>
	</rule>
	<rule>
		<from>/sfa.url</from>
		<to>/frontAnnouncement!getSingleFavor.action</to>
	</rule>








</urlrewrite> 